---
const { title } = Astro.props as { title: string };
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <title>{ title }</title>
    <style>
      body {
        background-color: #36393f;
        color: #bbbbbb;
      } 
    </style>
  </head>

  <body>
    <slot />
  </body>
</html>